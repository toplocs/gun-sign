{
  "name": "@gun-sign/server",
  "version": "1.0.0",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "test": "node tests/test_gun_setup.js",
    "test:gun": "node tests/test_gun_setup.js",
    "test:shims": "node tests/test_shims.js",
    "test:all": "npm run test:gun && npm run test:shims",
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:test": "node server.js --test",
    "debug": "node --inspect server.js",
    "monitor": "node --trace-warnings server.js",
    "benchmark": "node --expose-gc server.js"
  },
  "keywords": [
    "gun",
    "gundb",
    "database",
    "decentralized",
    "realtime"
  ],
  "author": "",
  "license": "MIT",
  "description": "A minimal GunDB server using the latest version directly from GitHub",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "ws": "^7.2.1",
    "gun": "workspace:*"
  }
}
